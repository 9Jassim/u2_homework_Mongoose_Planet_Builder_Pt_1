<%- include('../partials/header') %>

<h2>Explorers List</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
    </tr>
  </thead>
  <tbody>
    <% explorers.forEach(explorer => { %>
      <tr>
        <td><%= explorer.name %></td>
        <td><%= explorer.age %></td>
        </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>